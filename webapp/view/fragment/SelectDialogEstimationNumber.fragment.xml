<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<!-- 
		cancel=".handleBrandFragmentClose"
	 -->
    <TableSelectDialog
		noDataText="{i18n>noData}"
		title="{i18n>estimationNumber}"
		search=".onSearchestimationNumber"
		confirm=".onSelectestimationNumber"
		showClearButton="true"
		items= "{path : 'mCreateOrder>/testData'}"
		contentHeight= "30%"
		contentWidth="30%"
		resizable="true">
		<ColumnListItem>
			<cells>
                <Text text="{mCreateOrder>cod}"/>
                <Text text="{mCreateOrder>nameOrder}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column minScreenWidth="Tablet" demandPopin="true" width="12em">
				<header>
					<Text text="{i18n>code}" />
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true" width="12em">
				<header>
					<Text text="{i18n>description}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
