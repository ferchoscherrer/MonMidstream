<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>stimateNumber}"
		class="sapUiPopupWithPadding"
		items="{mCreateOrder>/testData}"
		confirm="handleListTroubleSelect"
		search="onSearchListStimateNumber"
		contentHeight="50%"
		contentWidth="30%">
		<StandardListItem
			title="{mCreateOrder>nameOrder}"
			info="{mCreateOrder>cod}" 
        />
	</SelectDialog>
</core:FragmentDefinition>