{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.triiari.retrobilling",
    "type": "application",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": [
        "es"
      ],
      "fallbackLocale": "es"
    },
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.15.6",
      "toolsId": "acdf45e0-eb98-4a0d-a707-782f020b4345"
    },
    "dataSources": {
      "createOrder": {
        "uri": "model/createOrder.model.json",
        "type": "JSON"
      },
      "modifyOrder": {
        "uri": "model/modifyOrder.model.json",
        "type": "JSON"
      },
      "ZGW_MAINSTREAM_CATALOGS_SRV": {
        "uri": "srvMonRetro/ZGW_MAINSTREAM_CATALOGS_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "ZSD_SALES_DOC_GET_SRV_01": {
        "uri": "srvMonRetro/ZSD_SALES_DOC_GET_SRV_01/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "ZSD_SALES_GET_DOC_SRV": {
        "uri": "srvMonRetro/ZSD_SALES_GET_DOC_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "ZSD_SALES_CREATE_DOC_SRV_01": {
        "uri": "srvMonRetro/ZSD_SALES_CREATE_DOC_SRV_01/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "retrobill-Display": {
          "semanticObject": "retrobill",
          "action": "Display",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.131.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.ui.table": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.triiari.retrobilling.i18n.i18n",
          "supportedLocales": [
            "es"
          ],
          "fallbackLocale": "es"
        }
      },
      "mCreateOrder": {
        "dataSource": "createOrder",
        "type": "sap.ui.model.json.JSONModel"
      },
      "mModifyOrder": {
        "dataSource": "modifyOrder",
        "type": "sap.ui.model.json.JSONModel"
      },
      "ZGW_MAINSTREAM_CATALOGS_SRV": {
        "dataSource": "ZGW_MAINSTREAM_CATALOGS_SRV",
        "preload": true,
        "settings": {
          "useBatch": true
        }
      },
      "ZSD_SALES_DOC_GET_SRV_01": {
        "dataSource": "ZSD_SALES_DOC_GET_SRV_01",
        "preload": true,
        "settings": {
          "useBatch": true
        }
      },
      "ZSD_SALES_GET_DOC_SRV": {
        "dataSource": "ZSD_SALES_GET_DOC_SRV",
        "preload": true,
        "settings": {
          "useBatch": true
        }
      },
      "ZSD_SALES_CREATE_DOC_SRV_01": {
        "dataSource": "ZSD_SALES_CREATE_DOC_SRV_01",
        "preload": true,
        "settings": {
          "useBatch": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.triiari.retrobilling.view",
        "async": true,
        "viewPath": "com.triiari.retrobilling.view"
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "RouteDetailSalesOrder",
          "pattern": "detail/{estimationNumber}{?query}",
          "target": [
            "TargetMain",
            "TargetDetailSalesOrder"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "id": "Main",
          "name": "Main",
          "level": 1
        },
        "TargetDetailSalesOrder": {
          "id": "DetailSalesDocument",
          "name": "DetailSalesDocument",
          "level": 2
        }
      }
    },
    "rootView": {
      "viewName": "com.triiari.retrobilling.view.App",
      "type": "XML",
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "comtriiariretrobilling"
  }
}
